fun main() : int = 
    let 
        var x : int
        var c : ^char
    in
        x = 1,
        x = x + 1,
        c = "aa",
        abc(k(1), k(2), k(3)),
        x = k(k(k(1))),
        return 0
    end

fun k(o : int): int =
    return o + 1


fun abc(t : int, p : int, o : int) : int =
    return t + p + o

fun infVoid() : int =
    while 1 == 1 do
        1 + 1
    end